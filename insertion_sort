#include <stdio.h>

#define LEN 5
int a[LEN] = { 10, 5, 2, 4, 7 };

void insertion_sort (void)
{
	printf("%d\t%d\t%d\t%d\t%d\n"
		,a[0],a[1],a[2],a[3],a[4]);
	int i, j, key,length;
	length = sizeof(a)/sizeof(int);
	printf("length is %d\n",length);
	for (i = 1; i < length; ++i)
	{
		key = a[i];
		j = i-1;	
		while (j>=0 && a[j]>key)
		{
		
			a[j+1] = a[j];
			--j;		
		}
		a[j+1] = key;	
	}
	printf("%d\t%d\t%d\t%d\t%d\n",a[0],a[1],a[2],a[3],a[4]);	
}

int main(void)
{
	insertion_sort();
	return 0;
}
